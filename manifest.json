{
  "name": "BoilerKey - Chrome",
  "version": "1.1",
  "permissions": [
    "storage",
    "cookies",
    "https://api-1b9bef70.duosecurity.com/*",
    "https://www.purdue.edu/apps/account/cas/login"
  ],
  "description": "A simple chrome extension that allows you to get your boilerkey passcode in browser",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "BoilerKey - In Browser",
    "default_popup": "dropdown.html"
  },
  "background": {
    "scripts": [
      "js/hotp.js",
      "js/boilerkey.js",
      "js/background.js"
    ]
  },
  "content_scripts": [{
    "matches": ["https://www.purdue.edu/apps/account/cas/login"],
    "js": [
      "js/hotp.js",
      "js/boilerkey.js",
      "js/dropdown.js"
    ]
  }],
  "icons": {
    "128": "key.png"
  }
}
